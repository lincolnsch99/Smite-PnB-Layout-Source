<UserControl x:Class="Smite_PnB_Layout.BanDisplay"
             x:Name="BanControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Smite_PnB_Layout"
             mc:Ignorable="d" 
             d:DesignHeight="143" d:DesignWidth="57" SizeChanged="UserControl_SizeChanged">
    <Canvas x:Name="canvas">
        <MediaElement x:Name="soundPlayer" LoadedBehavior="Manual" Source="Achilles.mp3" MediaFailed="soundPlayer_MediaFailed"></MediaElement>
        <Image x:Name="godImage" Height="143" Width="57" Canvas.Top="0" Canvas.Left="0" Stretch="Fill">
            <Image.Source>
                <BitmapImage UriSource=""/>
            </Image.Source>
        </Image>
    </Canvas>
</UserControl>
